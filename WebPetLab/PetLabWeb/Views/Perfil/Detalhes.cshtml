@model Domain.Entidade.Usuario

@{
    ViewData["Title"] = "Detalhes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<main class="page-content">
    <h1>Detalhes</h1>

    <div>
        <h4>Usuario</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nome)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Sobrenome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Sobrenome)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Telefone)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Telefone)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Cpf)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cpf)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Cnpj)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cnpj)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Crm)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Crm)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DataNascimento)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DataNascimento)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ImagemUrlusuario)
            </dt>
            <dd class="col-sm-10">
                <img src="@Model.ImagemUrlusuario" width="320px" height="320px" />
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CreatedAt)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CreatedAt)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.UpdatedAt)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.UpdatedAt)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.TipoUsuario)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TipoUsuario)
            </dd>
        </dl>
    </div>
    <div>
        @Html.ActionLink("Edit", "Editar", new { id = @Model.Id }) |
        <a asp-controller="Home" asp-action="Home">Voltar para o Home</a>
    </div>

</main>